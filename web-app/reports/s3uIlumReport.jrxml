	<subDataset name="New Dataset 2" uuid="8e8c1243-55de-4365-84fb-9e1b88b5401d">
		<queryString language="SQL">
			<![CDATA[select o.id, ts.tipo_servico, oc.numero_poste, e.numero, e.referencia, l.logradouro,
       b.bairro, c.cidade, es.estado, o.data_emissao, o.interessado, o.telefone, o.descricao, l.cep,
       (l.logradouro || ' - ' || b.bairro || ' - ' || l.cep) as endereco,
       TO_CHAR(o.data_emissao, 'DD/MM/YYYY') as data_os
  from administracao_ordem_de_servico.ordem_de_servico o,
              administracao_ordem_de_servico.complemento_Iluminacao oc,
              administracao_ordem_de_servico.tipo_servico ts,
              administracao_ordem_de_servico.servico s,
              administracao_ordem_de_servico.endereco e,
              administracao_ordem_de_servico.logradouro l,
              administracao_ordem_de_servico.bairro b,
              administracao_ordem_de_servico.cidade c,
              administracao_ordem_de_servico.estado es
where o.id = oc.ordem_de_servico_id
  and s.id = ts.servico_id
  and o.tipo_servico_id = ts.id
  and o.endereco_id = e.id
  and l.id = e.logradouro_id
  and b.id = l.bairro_id
  and b.cidade_id = c.id
  and es.id = c.estado_id]]>
		</queryString>
